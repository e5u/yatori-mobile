<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude logs from cloud backup -->
        <exclude domain="file" path="logs/"/>
        <exclude domain="cache" path="."/>
    </cloud-backup>
    <device-transfer>
        <exclude domain="file" path="logs/"/>
        <exclude domain="cache" path="."/>
    </device-transfer>
</data-extraction-rules>
